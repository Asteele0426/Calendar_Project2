<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th>Event Name</th>
      <th>Event Date</th>
    </tr>
  </thead>
  <tbody>
    <% event.forEach(function(e) { %>
      <tr>
        <td><%= e.eventName %></td>
        <td><%= e.eventDate %></td>
        <td><a href="events/<%= e._id %>">DETAILS</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>
